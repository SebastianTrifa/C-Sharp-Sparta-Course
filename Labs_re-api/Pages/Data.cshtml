@page
@model Labs_ReDoASP.Models.ClassesModel
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@

<div>
    <p>A list of our customers</p>
</div>

<table class="table table-hover table-striped table-bordered">
    <tr>
        <th>Name</th>
        <th>Company</th>
    </tr>
    @for(int i=0; i<=Model.name.Count-1; i++)
    {
    <tr>
        <td>@Model.name[i]</td>
        <td>@Model.company[i]</td>
    </tr>
    }
</table>
